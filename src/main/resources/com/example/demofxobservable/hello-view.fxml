<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="333.0" prefWidth="776.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demofxobservable.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox VBox.vgrow="ALWAYS">
      <children>
         <VBox prefWidth="441.0" HBox.hgrow="ALWAYS">
            <children>
               <ListView fx:id="llisViewCosas" prefWidth="200.0" VBox.vgrow="ALWAYS" />
            </children>
         </VBox>
         <VBox alignment="CENTER" prefHeight="209.0" prefWidth="441.0">
            <children>
               <VBox fx:id="vboxContenidos" alignment="CENTER" spacing="10.0" style="-fx-background-color: #FF9691;" VBox.vgrow="ALWAYS">
                  <children>
                     <StackPane fx:id="boxStackPane" style="-fx-background-color: #91E300;" VBox.vgrow="ALWAYS" />
                  </children></VBox>
            </children>
         </VBox>
      </children>
   </HBox>
   <HBox prefHeight="100.0" prefWidth="200.0" spacing="5.0">
      <children>
         <Button fx:id="btnAccion" mnemonicParsing="false" onAction="#btnAccionClick" text="DEL CHILD" />
         <Button fx:id="btnAccion2" mnemonicParsing="false" onAction="#btnAccion2Click" text="ADD TO CHILD" />
         <Button fx:id="btnAddBox" mnemonicParsing="false" onAction="#btnAddBoxClick" text="Add VBox" />
      </children>
   </HBox>
</VBox>
