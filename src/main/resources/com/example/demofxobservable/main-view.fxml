<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="558.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demofxobservable.MainController">
   <children>
      <HBox layoutX="-2.0" prefHeight="558.0" prefWidth="900.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox HBox.hgrow="ALWAYS">
               <children>
                  <ListView fx:id="lstNames" prefWidth="406.0" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="558.0" spacing="20.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label fx:id="lblName" text="Name" />
                  <TextField fx:id="txtInsert" prefHeight="40.0" VBox.vgrow="ALWAYS" />
                  <Button fx:id="btnInsert" mnemonicParsing="false" onAction="#btnInsertClick" prefHeight="51.0" prefWidth="213.0" text="INSERT" />
                  <TextField fx:id="txtUpdate" prefHeight="40.0" />
                  <Button fx:id="btnUpdate" mnemonicParsing="false" onAction="#btnUpdateClick" prefHeight="51.0" prefWidth="213.0" text="UPDATE" />
                  <Button fx:id="btnDelete" mnemonicParsing="false" onAction="#btnDeleteClick" prefHeight="51.0" prefWidth="213.0" text="DELETE" />
                  <Button fx:id="btnClickMe" mnemonicParsing="false" onAction="#btnClickMe" prefHeight="51.0" prefWidth="200.0" text="LOAD FROM DB" VBox.vgrow="ALWAYS" />
               </children>
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
